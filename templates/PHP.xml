<templateSet group="PHP">
  <template name="eco" value="echo '$END$';" description="'echo' statement" toReformat="true" toShortenFQNames="true">
    <context />
  </template>
  <template name="tryc" value="try {&#10;    $SELECTION$&#10;} catch (Exception $ex) {&#10;    $END$&#10;}" description="try-catch block" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="ext" value="exit('$END$');" toReformat="false" toShortenFQNames="true" />
  <template name="pubf" value="public function $NAME$($PARAMS$) {&#10;&#9;$END$&#10;}" description="public function" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="PARAMS" expression="" defaultValue="" alwaysStopAt="true" />
    <context />
  </template>
</templateSet>